  transition: all .2s ease-in-out;
  float: right;
  display: block;
  width: 100px;
  height: 80px;
  margin: 5px 15px 0 0;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  -webkit-background-size: contain;
  background-size: contain;
  text-indent: -9999in;
  margin-bottom: 10px;

  &:hover, &:focus {
    opacity: .7;
  }
}

/*----------  modal-size-chart  ----------*/

.modal-size-chart {
  .container-fluid {
    padding-left: 15px;
    padding-right: 15px;
  }
  thead {
    color: #000;
  }
  strong {
    font-weight: 900;
    color: {{ settings.link_color }};
  }
}

.modal-size-chart__logo {
}

.modal-size-chart__head {

  padding-top: $gutter/2;
  text-align: center;

  @media (min-width: $screen-sm-min) {
    @include flexbox();
    @include align-items(center);
    @include justify-content(space-between);
    text-align: left;
  }

  h1 {
    @include flex(1);
    line-height: 60px;
    font-weight: 900;
    font-size: 28px;
    margin: initial;
  }
}

.modal-size-chart__subhead {
  border-top: 1px dashed #ccc;
  border-bottom: 1px dashed #ccc;
  text-transform: uppercase;
  text-align: center;
  line-height: 40px;
  font-weight: 700;
  font-size: 16px;
  color: {{ settings.link_color }};

  @media (min-width: $screen-sm-min) {
    text-align: left;
  }
}

.modal-size-chart__chart {
  padding-top: 30px;
  padding-bottom: 20px;
}

.modal-size-chart__table {
  th {
    text-align: center;
    &:first-child {
      text-align: left;
    }
  }
}

.modal-size-chart__image {
  margin-bottom: 20px;
}



// Free shipping popup
#modal-free-shipping {
  max-width: 850px;
  background: #fff;
  padding: 15px;
  text-align: left;
  margin: 40px auto;
  position: relative;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  box-shadow: 0 1px 16px rgba(11,25,28,0.8);

  p {
    margin-top: 0.9em;
    margin-bottom: 0.9em;
  }

  div.container {padding: 0}
  @media only screen and (min-width: 768px) {
    .lead {font-size: 19px;}
  }

  @media only screen and (max-width: 768px) {
    .container div {
    	padding: 0 !important;
    }
  }
}

.mfp-container {
	@media only screen and (max-width: 768px) {
    padding: 0 20px;
  }
}

.free-shipping-popup-container {
  padding: 10px 0 $gutter;
}

.free-shipping-popup-toggle {

  @include flexbox();
  @include align-items(center);
  display: inline-flex;
  img {
    margin-right: 10px;
    height: 40px;
  }
  strong {
    display: block;
  }
  small {
    text-decoration: underline;
    color: #898989;
  }
  &:hover {
    small {
      text-decoration: none;
    }
  }
}
